<!-- Edit Modal -->
<div class="modal fade" id="editmodal" tabindex="-1" role="dialog" aria-labelledby="write-a-reviewLabel" aria-hidden="true">
    <div class="modal-dialog modal-md" role="document">
        <div class="modal-content modal-content-review">
            <div class="modal-header">
                <h5 class="modal-title" id="write-a-reviewLabel">Edit Project</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body modal-body-review edit-projects">
                <div class="row m_-10 ">
                    <div class="col-lg-12 p_10">
                        <div class="form-group">
                            <label>Any updates to your project will be reflected automatically for all matched contractors to see.</label>
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label>Job Title</label>
                                    <input type="text" class="form-control InputValidation" id="JobTitle" data-parsley-pattern="/^[a-zA-Z \W ]*$/" required data-parsley-required-message="Job Title is Required" data-parsley-trigger="keyup">
                                </div>
                            </div>

                            <div class="col-md-6">
                                <div class="form-group">
                                    <label>Estimated Start Date</label>
                                    <input type="datetime" class="form-control InputValidation datepicker" id="EstimatedStartDate" required data-parsley-required-message="Estimated Start Date is Required" data-parsley-required="true" data-parsley-trigger="keyup">
                                </div>
                            </div>
                        </div>

                        <div class="form-group">
                            <label>Job Description</label>
<textarea class="form-control InputValidation" rows="5" id="JobDescription" required data-parsley-required-message="Job Description is Required" data-parsley-trigger="keyup"></textarea>
                        </div>

                        <div class="form-group">
                            <label>Estimated Budget</label>
                            @*<input type="number" class="form-control InputValidation" id="EstimatedBudget" min=0 oninput="validity.valid||(value='');" onchange="ClearBorder()">*@

                            <select class="form-control" id="EstimatedBudget" required data-parsley-required-message="You must select Estimated Budget">
                                <option value="">Estimated Budget</option>
                                <option value="$100 - $500">$100 - $500</option>
                                <option value="$500 - $1,000">$500 - $1,000</option>
                                <option value="$1,000 - $5,000">$1,000 - $5,000</option>
                                <option value="$5,000 - $20,000">$5,000 - $20,000</option>
                                <option value="$20,000 - $50,000">$20,000 - $50,000</option>
                                <option value="$50,000 - $100,000">$50,000 - $100,000</option>
                                <option value="$100,000 - $250,000">$100,000 - $250,000</option>
                                <option value="$250,000 - $500,000">$250,000 - $500,000</option>
                                <option value="$500,000 - $1,000,000">$500,000 - $1,000,000</option>
                                <option value="$1,000,000">$1,000,000</option>
                                <option value="Uncertain">Uncertain</option>
                            </select>

                        </div>



                    </div>
                    <div class="col-lg-12">
                        <h5>Project Upload</h5>
                        <div class="row ">
                            @*<h3>Images</h3>*@
                            <div class="col-md-12 Images"></div>
                            @*<h3>Documents</h3>*@
                            <div class="col-md-12 Documents">

                            </div>
                            <div class="no-upproject">

                            </div>
                        </div>
                        <div class="row">

                            @*<div class="btnContainer">
                                <div class="valign-middle multilefileselect">
                                <label for="file" class="sr-only"> Attach File(S)</label>
                                @*<input type="file" id="file">*-@
                                @*<input type="file" id="imageUploadForm" multiple name="filename">*-@
                                @*<input type="file" id="imageUploadForm" name="filename" accept=".png,.gif,.jpeg,.doc,.pdf,.xlsx" multiple="multiple" onchange="javascript:updateList()">*-@
                                <input type="file" id="imageUploadForm" name="filename" accept=".png,.gif,.jpeg,.doc,.pdf,.xlsx" multiple="multiple" onchange="javascript:updateList()">
                                @*<span style="color:red" id="filemsgformHome">Please Select File</span>*-@
                                <div id="fileListFromHome"><ul></ul></div>
                                <label id="lblProjectID" style="display:none;"></label>
                                </div>
                                </div>*@

                            <div class="btnContainer">
                                <div class="valign-middle multilefileselect">
                                    <label for="file" class="sr-only"> Attach File(S)</label>
                                    @*<input type="file" id="file">*@
                                    @*<input type="file" id="imageUploadForm" multiple name="filename">*@
                                    <input type="file" id="imageUploadForm" name="filename" accept=".png,.gif,.jpeg,.doc,.pdf,.xlsx" multiple="multiple" onchange="javascript:updateList()">
                                    <span style="color:red" id="filemsgformHome">Please Select File</span>
                                    <div id="fileListFromHome">
                                        <ul></ul>
                                    </div>
                                    <label id="lblProjectID" style="display:none;"></label>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                @*<button type="button" data-dismiss="modal" class="btn btn-primary btn-custom" id="btnUpdate">Save Changes</button>
                    <button type="button" data-dismiss="modal" class="btn btn-primary btn-custom" id="btnUpdateAttachments">Update Attachements</button>*@
                <button type="button" class="btn btn-primary btn-custom" id="SaveImageData">UPDATE PROJECT DETAILS</button>
            </div>

        </div>
    </div>
</div>
