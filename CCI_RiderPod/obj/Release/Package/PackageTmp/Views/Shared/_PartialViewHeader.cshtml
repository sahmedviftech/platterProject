                @if (user != null)
                {
                    <div class="col-lg-3 order-lg-7 col-sm-12">
                        <div class="dropdown custom-user text-rights">
                            <a class=" dropdown-toggle" href="#" role="button" id="dropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                @*<span><img src="~/Content/assets/img/user.png" alt=""></span>*@ 
                                <span class="user-name">@user.FirstName </span>
                            </a>
                            <div class="dropdown-menu" aria-labelledby="dropdownMenuLink">
                                @*<a class="dropdown-item" href="#">Action</a>
                                <a class="dropdown-item" href="#">Another action</a>
                                    onclick='window.location="@Url.Action("Logout", "Login")";' *@
                                @if (user.AccountTypeID == 1)
                                {
                                    <a class="dropdown-item" href="/Favorite/Index" ;><i class="fa fa-sign-out" aria-hidden="true"></i>Favourites</a>
                                }
                                @if (user.AccountTypeID != 4)
                                {

                                    <a class="dropdown-item" href="/Incentive/Index?id=@user.Id" ;><i class="fa fa-sign-out" aria-hidden="true"></i>EARN CASH</a>
                                }
                                @if (user.AccountTypeID == 2)
                                {

                                    <a class="dropdown-item" href="/BillingHistory/Index"><i class="fa fa-sign-out" aria-hidden="true"></i>Billing History</a>
                                    <a class="dropdown-item" href="/Contractor/ContractorDetail?id=@user.Id"><i class="fa fa-sign-out" aria-hidden="true"></i>Public Profile    <span class="active-reviews" id="CountNewListReview" style="display:none"></span></a>
                                    @*<a class="dropdown-item" href="/Profiles/ContractorProfile?userId=@user.Id"><i class="fa fa-sign-out" aria-hidden="true"></i>Edit Profile</a>*@
                                }
                                @if (user.AccountTypeID == 1)
                                {
                                    <a class="dropdown-item" href="/Profiles/MyProfile" ;><i class="fa fa-sign-out" aria-hidden="true"></i>Edit Profile</a>
                                }
                                <a class="dropdown-item" href="" ; onclick="SessionLogout(@user.AccountTypeID)"><i class="fa fa-sign-out" aria-hidden="true"></i>Logout</a>
                            </div>
                        </div>
                    </div>

                }
                else
                {
                    <div class="col-lg-3 order-lg-7 col-sm-12">
                        <form class="form-login">
                            @*     <button type="button" class="btn btn-outline-primary custm-outline-btn" onclick='window.location.href = "homii.codecreators.io/signup/"'>Sign up</button>*@
                            <button type="button" class="btn btn-outline-primary custm-outline-btn" onclick="window.location = 'https://homii.ca/signup/';">Sign up</button>
                            <button type="button" class="btn btn-primary btn-custom" onclick='window.location = "@Url.Action("Index", "Login")";'>Login</button>
                        </form>
                    </div>
                }


                <div class="col-lg-7 h-0">
                    <div class="nav-form">
                        <form>
                            <div class="form-row">
                                <div class="input-group form-group">
                                    <div class="input-group-append">
                                        <span class="input-group-text form-label">Find <i class="fa fa-caret-right"></i></span>
                                    </div>
                                    @* <input type="text" class="form-control custom-form-control" id="CategoriesList" onblur="if(this.value == '') { this.value='e.g. Plumber, Roofer, Handyman'}" onfocus="if (this.value == 'e.g. Plumber, Roofer, Handyman') {this.value=''}" value="e.g. Plumber, Roofer, Handyman" autocomplete="off">*@
                                    <input type="text" class="form-control custom-form-control" id="CategoriesList" placeholder="e.g. Plumber, Roofer, Handyman" onkeyup="ClearBorder()" autocomplete="off">
                                    <div class="input-group-append">
                                        <span class="input-group-text form-label">Near <i class="fa fa-caret-right"></i></span>
                                    </div>
                                    @*<input type="text" class="form-control custom-form-control" id="CitiesList" onblur="if(this.value == '') { this.value='e.g. Toronto, ON'}" onfocus="if (this.value == 'e.g. Toronto, ON') {this.value=''}" value="e.g. Toronto, ON" autocomplete="off">*@
                                    <input type="text" class="form-control custom-form-control" id="CitiesList" placeholder="e.g. Toronto, ON" onkeyup="ClearBorder()" autocomplete="off">
                                    <div class="input-group-append" id="SearchContractorByCat">
                                        @*<a href="/SearchContractor/Index" class="input-group-text btn btn-primary btn-custom btn-search-icon"><i class="fa fa-search"></i></a>*@
                                        <a class="input-group-text btn btn-primary btn-custom btn-search-icon"><i class="fa fa-search"></i></a>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
