<div class="testimonials">
    <div class="container">
        <div class="row align-items-end">
            <div class="col-lg-5">
                <div class="main-HD">
                    <h1>Report List </h1>
                </div>
            </div>
            <div class="col-lg-7">
                <div class="pro-details">
                    <form class="row align-items-end justify-content-end pdng_top-10">
                        <div class="col-md-8 pl-lg-0">
                            <div class="project-tab">
                                <ul class="pro-ul mb-3 mb-md-0">
                                    <li class="tab-slider--trigger pro-active show-tab CursorPointer">Homeowner Reports</li>
                                    <li class="tab-slider--trigger hide-tab CursorPointer">Contractor Reports</li>
                                </ul>
                            </div>
                        </div>
                       
                    </form>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-12">
                <div class="custom-table tbl-show cstm-reports">
                    <div class="table-responsive">
                        <table class="table" id="example">
                            <thead class="thead-dark">
                                <tr>
                                   @* <th scope="col" class="th-width-1">ID</th>*@
                                    <th scope="col" class="th-width-2">Contractor Name</th>
                                    <th scope="col" class="th-width-3">Subject</th>
                                    <th scope="col" class="th-width-4">Description</th>
                                </tr>
                            </thead>

                        </table>
                    </div>
             
                </div>
                <div class="custom-table tbl-hide cstm-reports">
                    <div class="table-responsive-lg">
                        <table class="table" id="example1">
                            <thead class="thead-dark">
                                <tr>
                                    @* <th scope="col" class="th-width-1">ID</th>*@
                                    <th scope="col" class="th-width-2">Homeowner Name</th>
                                    <th scope="col" class="th-width-3">Subject</th>
                                    <th scope="col" class="th-width-4">Description</th>
                                </tr>
                            </thead>
                            <tbody id="tbodyComplete"></tbody>
                        </table>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>




