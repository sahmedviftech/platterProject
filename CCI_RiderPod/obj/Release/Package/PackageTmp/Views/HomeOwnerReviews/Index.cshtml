<div class="modal fade" id="editmodal" tabindex="-1" role="dialog" aria-labelledby="write-a-reviewLabel" aria-hidden="true">
    <div class="modal-dialog modal-md" role="document">
        <div class="modal-content modal-content-review">
            <div class="modal-header">
                <h5 class="modal-title" id="write-a-reviewLabel">Edit Homeowner Reviews</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body modal-body-review">
                <div class="row m_-10 editprofile">
                    <div class="col-md-6 p_10">
                        <input id="ReviewId" name="ReviewId" value="" hidden>
                        <div class="form-group">
                            <label>Homeowner Name</label>
                            <input type="text" class="form-control" required data-parsley-trigger="keyup" data-parsley-required-message="Homeowner Name is required" id="HomeOwnerName" value="" readonly>
                        </div>
                    </div>

                    <div class="col-md-6 p_10">
                        <div class="form-group">
                            <label>Contractor Name</label>
                            <input type="text" required data-parsley-trigger="keyup" data-parsley-required-message="Contractor Name is required" class="form-control" id="ContractorName" value="" readonly>
                        </div>
                    </div>

                    <div class="col-md-12 p_10">
                        <div class="form-group">
                            <label>Review Title</label>
                            <input type="text" required data-parsley-trigger="keyup" data-parsley-required-message="Review Title Name is required" class="form-control" id="Title" value="">
                        </div>
                    </div>
                    <div class="col-md-12 p_10">
                        <div class="form-group">
                            <label>Review Description</label>
                            <input type="text" class="form-control" required data-parsley-trigger="keyup" data-parsley-required-message="Description is required" id="Description" value="">
                        </div>
                    </div>
                    <div class="col-md-6 p_10">
                        <div class="form-group">
                            <label> QualityOfWork </label><br>
                            <input type="number" class="form-control" id="QualityOfWork" min="1" max="5" value="" required data-parsley-trigger="keyup" data-parsley-required-message="Quality of work is required">
                            @*<div class="rateit" data-rateit-mode="font" id="QualityOfWork" data-rateit-value="" data-rateit-step="1"></div>*@
                        </div>
                    </div>
                    <div class="col-md-6 p_10">
                        <div class="form-group">
                            <label> Timeliness </label><br>
                            <input type="number" class="form-control" id="Timeliness" min="1" max="5" value="" required data-parsley-trigger="keyup" data-parsley-required-message="Timeliness  is required">
                            @*<div class="rateit" data-rateit-mode="font" id="Timeliness" data-rateit-value="" data-rateit-step="1"></div>*@
                        </div>
                    </div>

                    <div class="col-md-6 p_10">
                        <div class="form-group">
                            <label> StayedInBudget </label><br>
                            <input type="number" class="form-control" id="StayedInBudget" min="1" max="5" value="" required data-parsley-trigger="keyup" data-parsley-required-message="StayedinBudget  is required">
                            @*<div class="rateit" data-rateit-mode="font" id="StayedInBudget" data-rateit-value="" data-rateit-step="1"></div>*@
                        </div>
                    </div>
                    <div class="col-md-6 p_10">
                        <div class="form-group">
                            <label> Communication </label><br>
                            <input type="number" class="form-control" id="Communication" min="1" max="5" value="" required data-parsley-trigger="keyup" data-parsley-required-message="Communication is required">
                            @*<div class="rateit" data-rateit-mode="font" id="Communication" data-rateit-value="" data-rateit-step="1"></div>*@
                        </div>
                    </div>

                    <div class="col-md-6 p_10">
                        <div class="form-group">
                            <label> Cleanliness </label><br>
                            <input type="number" class="form-control" id="Cleanliness" min="1" max="5" value="" required data-parsley-trigger="keyup" data-parsley-required-message="Cleanliness is required">
                            @*<div class="rateit" data-rateit-mode="font" id="Cleanliness" data-rateit-value="" data-rateit-step="1"></div>*@
                        </div>
                    </div>
                    <div class="col-md-6 p_10">
                        <div class="form-group">
                            <label> Courteous </label><br>
                            <input type="number" class="form-control" id="Courteous" min="1" max="5" value="" required data-parsley-trigger="keyup" data-parsley-required-message="Courteous is required">
                            @*<div class="rateit" data-rateit-mode="font" id="Courteous" data-rateit-value="" data-rateit-step="1"></div>*@
                        </div>
                    </div>





















                </div>
            </div>
            <div class="modal-footer">
                <button type="button" id="Updatebtn"  class="btn btn-primary btn-custom">Save Changes</button>
            </div>
        </div>
    </div>
</div>



