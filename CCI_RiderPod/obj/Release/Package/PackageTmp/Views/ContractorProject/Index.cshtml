<!--Complete Modal -->
<div class="modal fade" id="Completemodal" tabindex="-1" role="dialog" aria-labelledby="CompletemodalLabel" aria-hidden="true">
    <div class="modal-dialog modal-md" role="document">
        <div class="modal-content modal-content-review">
            <div class="modal-header">
                <h5 class="modal-title" id="write-a-reviewLabel">Mark as working</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body modal-body-review">
                <p>Marking a project as Working, will move it into another bucket, to help you manage your jobs. This is for your own reference ONLY and will not impact the project on the homeowner's end</p>
            </div>
            <div class="modal-footer">
                <button type="button" data-dismiss="modal" id="CompleteId" class="btn btn-primary btn-custom">Yes</button>
                <button type="button" data-dismiss="modal" class="btn btn-primary btn-custom">No</button>
            </div>
        </div>
    </div>
</div>





<!--Delete Modal -->
<div class="modal fade" id="deletemodal" tabindex="-1" role="dialog" aria-labelledby="write-a-reviewLabel" aria-hidden="true">
